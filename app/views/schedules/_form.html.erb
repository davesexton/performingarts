<%= form_for(@schedule) do |f| %>
  <% if @schedule.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@schedule.errors.count, "error") %> prohibited this schedule from being saved:</h2>

      <ul>
      <% @schedule.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="field">
    <%= f.label :age_group_id %><br />
    <%= f.select(:age_group_id,
                 options_from_collection_for_select(AgeGroup.all,
                                                    :id,
                                                    :name,
                                                    f.object.age_group_id),
                  {:include_blank => true})  %>
  </div>
  <div class="field">
    <%= f.label :course_id %><br />
    <%= f.select(:course_id,
                 options_from_collection_for_select(Course.all,
                                                    :id,
                                                    :name,
                                                    f.object.course_id),
                 {include_blank: true}) %>
  </div>
  <div class="field">
    <%= f.label :venue_id %><br />
    <%= f.select(:venue_id,
                  options_from_collection_for_select(Venue.all,
                                                     :id,
                                                     :name,
                                                     f.object.venue_id),
                  {include_blank: true}) %>
  </div>
  <div class="field">
    <%= f.label :time_start %><br />
    <%= f.time_select :time_start, include_blank: true %>
  </div>
  <div class="field">
    <%= f.label :time_end %><br />
    <%= f.time_select :time_end, include_blank: true %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
